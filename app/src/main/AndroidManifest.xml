<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.learning2">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name = "android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_mine"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_mine_round"
        android:supportsRtl="true"
        android:requestLegacyExternalStorage="true"
        android:theme="@style/Theme.Learning2">
        <activity android:name=".Splash_Activity">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />

                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".Subjects">
        </activity>
        <activity android:name=".Schedules">
        </activity>
        <activity android:name=".NotificationActivity">
        </activity>
        <receiver android:name= ".DoItNotificationPublisher" />
    </application>

</manifest>